{
    "name": "cloudflare-desktop-panel",
    "version": "1.0.0",
    "description": "Premium Desktop Control Panel for Cloudflare",
    "repository": {
        "type": "git",
        "url": "https://github.com/DM-WuzuanTW/cloudflare.git"
    },
    "author": "DM-WuzuanTW",
    "main": "electron/main.js",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "electron:dev": "vite build && electron .",
        "electron:start": "electron .",
        "dist": "vite build && electron-builder"
    },
    "build": {
        "appId": "com.cloudflare.desktop",
        "productName": "Cloudflare Desktop",
        "directories": {
            "output": "release"
        },
        "files": [
            "dist/**/*",
            "electron/**/*"
        ],
        "win": {
            "target": "nsis",
            "icon": "build/icon.png"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "deleteAppDataOnUninstall": true
        },
        "publish": [
            {
                "provider": "github",
                "owner": "DM-WuzuanTW",
                "repo": "cloudflare"
            }
        ]
    },
    "keywords": [],
    "license": "ISC",
    "dependencies": {
        "axios": "^1.6.2",
        "electron-updater": "^6.6.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.5.0"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.2.0",
        "electron": "^28.0.0",
        "electron-builder": "^24.9.1",
        "vite": "^5.0.0"
    }
}
